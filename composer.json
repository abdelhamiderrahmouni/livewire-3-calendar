{
    "name": "abdelhamiderrahmouni/livewire-3-calendar",
    "description": "Laravel Livewire calendar component",
    "keywords": [
        "livewire",
        "calendar",
        "livewire-calendar",
        "livewire-3-calendar"
    ],
    "homepage": "https://github.com/abdelhamiderrahmouni/livewire-3-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Abdelhamid Errahmouni",
            "email": "abdelhamiderrahmouni@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AbdelhamidErrahmouni\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbdelhamidErrahmouni\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbdelhamidErrahmouni\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ],
            "aliases": {
                "LivewireCalendar": "AbdelhamidErrahmouni\\LivewireCalendar\\LivewireCalendarFacade"
            }
        }
    }
}
